<Application x:Class="afishaParser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:afishaParser"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Styles/Colors.xaml"/>
				<ResourceDictionary Source="Styles/Fonts.xaml"/>
				<ResourceDictionary Source="Styles/Texts.xaml"/>
				<ResourceDictionary Source="Styles/Buttons.xaml"/>
				<ResourceDictionary Source="Styles/Windows.xaml"/>
				<ResourceDictionary Source="Styles/TextBox.xaml"/>
				<ResourceDictionary Source="Styles/ListBox.xaml"/>
				<ResourceDictionary Source="Styles/Images.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
    </Application.Resources>
</Application>
